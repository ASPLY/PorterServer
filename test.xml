<?xml version='1.0' encoding='UTF-8'?>
<dataset>
  <table name="ARTICLES">
    <column>ID</column>
    <column>IS_SOLD</column>
    <column>USER_ID</column>
    <column>NAME</column>
    <column>MIDDLE_CATEGORY</column>
    <column>LARGE_CATEGORY</column>
    <column>PRICE</column>
    <column>STATE</column>
    <column>QUANTITY</column>
